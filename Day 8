class Solution {
public:
    int search(vector<int>& nums, int target) {
        int s =0;
        int e = nums.size()-1;
        int mid = s + (e-s)/2;

        for(int i=0; i<nums.size();i++){
            if(target == nums[i]){
                return i;
            }
        }
        return -1;
    }
};
